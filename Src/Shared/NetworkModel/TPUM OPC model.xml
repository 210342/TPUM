        <DisplayName Key="Id">Id</DisplayName>
      </Property>
      <Property SymbolicName="q3:FirstName" ModellingRule="Optional" DataType="q4:String" ValueRank="Scalar" AccessLevel="ReadWrite">
        <BrowseName>FirstName</BrowseName>
