      <Object SymbolicName="q1:Author" TypeDefinition="q1:Author" ModellingRule="Optional">
        <BrowseName>Author</BrowseName>
        <DisplayName Key="Author">Author</DisplayName>
      </Object>
    </Children>
    <ClassName>Book</ClassName>
