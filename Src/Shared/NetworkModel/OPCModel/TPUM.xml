<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="OPCUAModelDesign.xslt"?>
<ModelDesign xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" TargetNamespace="http://tpum.example.com" TargetXmlNamespace="http://tpum.example.com" xmlns="http://opcfoundation.org/UA/ModelDesign.xsd">
  <Namespaces>
    <Namespace Name="OpcUa" Prefix="Opc.Ua" XmlNamespace="http://opcfoundation.org/UA/2008/02/Types.xsd">http://opcfoundation.org/UA/</Namespace>
    <Namespace Name="TPUM" Prefix="TPUM" XmlNamespace="http://tpum.example.com" FilePath="TPUM">http://tpum.example.com</Namespace>
  </Namespaces>
  <DataType xmlns:q1="http://tpum.example.com" SymbolicName="q1:Book" xmlns:q2="http://opcfoundation.org/UA/" BaseType="q2:BaseDataType">
    <BrowseName>Book</BrowseName>
    <DisplayName Key="Book">Book</DisplayName>
    <Children>
      <Property SymbolicName="q1:Title" DataType="q2:String" ValueRank="Scalar" AccessLevel="ReadWrite">
        <BrowseName>Title</BrowseName>
        <DisplayName Key="Title">Title</DisplayName>
      </Property>
      <Property SymbolicName="q1:Id" ModellingRule="Mandatory" DataType="q2:Int32" ValueRank="Scalar" AccessLevel="ReadWrite">
        <BrowseName>Id</BrowseName>
        <DisplayName Key="Id">Id</DisplayName>
      </Property>
      <Property SymbolicName="q1:Authors" ModellingRule="Optional" DataType="q1:Author" ValueRank="Array" ArrayDimensions="1" AccessLevel="ReadWrite">
        <BrowseName>Authors</BrowseName>
        <DisplayName Key="Authors">Authors</DisplayName>
      </Property>
    </Children>
  </DataType>
  <DataType xmlns:q3="http://tpum.example.com" SymbolicName="q3:Author" xmlns:q4="http://opcfoundation.org/UA/" BaseType="q4:BaseDataType">
    <BrowseName>Author</BrowseName>
    <DisplayName Key="Author">Author</DisplayName>
    <Children>
      <Property SymbolicName="q3:Books" ModellingRule="Optional" DataType="q3:Book" ValueRank="Array" ArrayDimensions="1" AccessLevel="ReadWrite">
        <BrowseName>Books</BrowseName>
        <DisplayName Key="Books">Books</DisplayName>
      </Property>
      <Property SymbolicName="q3:Id" ModellingRule="Mandatory" DataType="q4:Int32" ValueRank="Scalar" AccessLevel="Write">
        <BrowseName>Id</BrowseName>
        <DisplayName Key="Id">Id</DisplayName>
      </Property>
      <Property SymbolicName="q3:FirstName" ModellingRule="Optional" DataType="q4:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>FirstName</BrowseName>
        <DisplayName Key="FirstName">FirstName</DisplayName>
      </Property>
      <Property SymbolicName="q3:LastName" ModellingRule="Optional" DataType="q4:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>LastName</BrowseName>
        <DisplayName Key="LastName">LastName</DisplayName>
      </Property>
      <Property SymbolicName="q3:NickName" ModellingRule="Optional" DataType="q4:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>NickName</BrowseName>
        <DisplayName Key="NickName">NickName</DisplayName>
      </Property>
    </Children>
  </DataType>
</ModelDesign>